{
  "appId": "com.ipro.app",
  "appName": "iPro",
  "webDir": "www",
  "bundledWebRuntime": false,
  "plugins": {
    "SplashScreen": {
      "launchShowDuration": 0,

      "launchAutoHide": true,

      "backgroundColor": "#041934",
      
      "showSpinner": false
    },
    "PushNotifications": {
      "presentationOptions": [
        "badge",
        "sound",
        "alert"
      ]
    },
    "server": {
      "allowNavigation": [
        "*"
      ]
    }
  }
}

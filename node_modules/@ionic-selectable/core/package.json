{
  "name": "@ionic-selectable/core",
  "title": "Ionic Selectable",
  "version": "5.0.0-alpha.13",
  "description": "An Ionic component similar to Ionic Select, that allows to search items, including async search, group, add, edit, delete items, and much more.",
  "author": {
    "name": "Evgenii Koriakin",
    "url": "https://github.com/ionic-selectable"
  },
  "license": "MIT",
  "homepage": "https://github.com/ionic-selectable/ionic-selectable",
  "repository": {
    "type": "git",
    "url": "https://github.com/ionic-selectable/ionic-selectable.git"
  },
  "bugs": {
    "url": "https://github.com/ionic-selectable/ionic-selectable/issues"
  },
  "keywords": [
    "ionic",
    "ionic 2",
    "ionic 3",
    "ionic 4",
    "ionic 5",
    "ion-select",
    "search",
    "searchable",
    "searchbar",
    "select",
    "selectable"
  ],
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "es2015": "dist/esm/index.js",
  "es2017": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/ionic-selectable/ionic-selectable.esm.js",
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "stencil build --docs",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate",
    "prettier": "prettier src/**/*.tsx src/**/*.scss --write",
    "prepare": "npm run build",
    "lint": "npm run lint.ts && npm run lint.sass",
    "lint.fix": "npm run lint.ts.fix && npm run lint.sass.fix",
    "lint.sass": "stylelint \"src/**/*.scss\"",
    "lint.sass.fix": "npm run lint.sass -- --fix",
    "lint.ts": "tslint --project .",
    "lint.ts.fix": "tslint --project . --fix"
  },
  "devDependencies": {
    "@ionic/core": "^5.6.3",
    "@stencil/core": "^2.5.2",
    "@stencil/sass": "^1.4.1",
    "prettier": "^2.2.1",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "tslint-react": "^5.0.0",
    "typescript": "~4.2.3"
  },
  "peerDependencies": {
    "@ionic/core": "5.x"
  },
  "publishConfig": {
    "directory": "/",
    "access": "public"
  },
  "gitHead": "1d149bf14d8cb0a991eb360dd2be622e2e6af254"
